{% extends 'base.html.twig' %}
{% block body %}
    <div class="container">
        <div class="row">
            {% for image in images %}
            <div class="col-md-6">
                <img src="/{{ image.path }}" class="img-fluid">
                <div class="text-center">{{ image.userEmail }}</div>
            </div>
            {% endfor %}
        </div>

    </div>
{% endblock %}